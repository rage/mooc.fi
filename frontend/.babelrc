{
  "presets": ["next/babel", "@zeit/next-typescript/babel"],
  "plugins": [
    ["styled-components", 
      {
        "ssr": true
      }
    ],
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "/": ".",
        "@static": "./static",
        "@pages": "./pages",
        "@lib": "./lib",
        "@components": "./components"
      },
      "extensions": [".ts", ".tsx", ".js", ".jpg", ".png", ".webp"]
    }],
    "lodash"
  ]
}
