    "@google-cloud/storage": "^3.0.2",
    "@types/compression": "^1.0.1",
    "@types/graphql-upload": "^8.0.0",
    "@types/lodash": "^4.14.136",
    "@types/luxon": "^1.15.2",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.14",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^3.4.5",
    "@types/yup": "^0.26.17",
    "apollo-server-core": "^2.5.0",
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^4.1.0",
    "dotenv-safe": "^6.1.0",
    "express": "^4.17.1",
    "graphql": "^14.3.0",
    "graphql-upload": "^8.0.7",
    "graphql-yoga": "^1.17.4",
    "lodash": "^4.17.11",
    "luxon": "^1.16.0",
    "micromustache": "^6.1.1",
    "mime-types": "^2.1.25",
    "mkdirp": "^0.5.1",
    "nexus": "^0.11.7",
    "nexus-prisma": "^0.3.7",
    "node-rdkafka": "^2.7.0",
    "nodemailer": "^6.3.1",
    "prisma": "^1.34.0",
