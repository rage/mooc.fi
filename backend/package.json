    "@types/luxon": "^1.24.0",
    "@types/morgan": "^1.9.0",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.22",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^8.0.0",
    "@types/websocket": "^1.0.0",
    "@types/yup": "^0.29.3",
    "JSONStream": "^1.3.5",
    "apollo-server-core": "^2.14.3",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "graphql-middleware": "^4.0.2",
    "graphql-upload": "^11.0.0",
    "knex": "^0.21.1",
