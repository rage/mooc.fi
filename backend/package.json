{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "test": "jest",
    "upgrade-interactive": "npm-check --update",
    "fetch-ai-completions": "node ./dist/bin/fetchAiCompletions.js",
    "fetch-organizations": "node ./dist/bin/importOrganizations.js",
    "fetch-user-app-datum": "node ./dist/bin/fetchUserAppDatum.js",
    "fetch-user-field-values": "node ./dist/bin/fetchUserFieldValues.js",
    "fetch-avoin-links": "node ./dist/bin/fetchAvoinLinks.js",
    "sync-tmc-users": "node ./dist/bin/syncTMCUsers.js",
    "send-ai-statistics": "node ./dist/bin/sendAiStatistics.js",
    "import-organizations": "node ./dist/bin/importOrganizations.js",
    "parse-file": "node ./dist/bin/parseFile.js",
    "start-kafka-consumers": "concurrently --kill-others --restart-tries 2 npm:kafka-consumer-*",
    "kafka-consumer-user-course-progress-batch": "node ./dist/bin/kafkaConsumer/userCourseProgressConsumer/kafkaConsumer.js",
    "kafka-consumer-user-course-progress-realtime": "node ./dist/bin/kafkaConsumer/userCourseProgressRealtimeConsumer/kafkaConsumer.js",
    "kafka-consumer-user-points-batch": "node ./dist/bin/kafkaConsumer/userPointsConsumer/kafkaConsumer.js",
    "kafka-consumer-user-points-realtime": "node ./dist/bin/kafkaConsumer/userPointsRealtimeConsumer/kafkaConsumer.js",
    "kafka-consumer-exercises": "node ./dist/bin/kafkaConsumer/exerciseConsumer/kafkaConsumer.js",
    "generate": "npm run generate:prisma && npm run generate:nexus",
    "generate:prisma": "prisma generate",
    "kafka-status": "node ./dist/bin/kafkaStatus.js",
    "seed": "node ./dist/bin/seed.js",
    "seedPoints": "node ./dist/bin/seedPoints.js",
    "seedExercises": "node ./dist/bin/seedExercises.js",
    "createExports": "ts-node ./bin/createExports.ts",
    "kafka-bridge": "node ./dist/bin/kafkaBridge.js",
    "background-emailer": "node ./dist/bin/backgroundEmailer.js",
    "update-course-statuses": "node ./dist/bin/updateCourseStatuses.js",
    "remove-duplicate-completions": "node ./dist/bin/removeDuplicateCompletions.js",
    "download-env": "node ./bin/downloadEnv.js",
    "dev": "npm run download-env && ts-node-dev --transpile-only ./app.ts",
    "dev:typecheck": "tsc --noEmit --watch",
    "generate:nexus": "env NEXUS_REFLECTION=true NODE_ENV=production ts-node --transpile-only ./schema.ts",
    "build": "npm run download-env && npm run createExports && tsc --noEmit false",
    "start": "node dist/app.js",
    "copy-env": "cp ./config/env.json ./dist/config/env.json",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "create-test-db": "env PGPASSWORD=prisma createdb -h localhost -p 5678 -U prisma -w testing || true",
    "build-sourcemap": "tsc --noEmit false --sourceMap --outDir sourcemap"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "^1.1.0",
