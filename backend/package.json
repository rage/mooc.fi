    "mime-types": "^2.1.27",
    "mkdirp": "^1.0.4",
    "morgan": "^1.10.0",
    "nexus": "^0.25.0",
    "nexus-plugin-moocfi-auth-plugin": "file:nexus-plugin-moocfi-auth-plugin",
    "nexus-plugin-prisma": "^0.16.1",
    "nexus-plugin-shield": "^0.1.4",
    "node-rdkafka": "^2.9.0",
    "nodemailer": "^6.4.8",
    "pg": "^8.2.1",
    "pg-query-stream": "^3.1.1",
    "redis": "^3.0.2",
    "sharp": "^0.25.4",
    "shortid": "^2.2.15",
    "uuid": "^8.1.0",
    "websocket": "^1.0.31",
    "winston": "^3.2.1",
