    "@types/morgan": "^1.9.2",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^8.3.0",
    "@types/websocket": "^1.0.1",
    "@types/yup": "^0.29.11",
    "apollo-server-express": "^2.19.1",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-scalars": "^1.7.0",
    "graphql-upload": "^11.0.0",
    "helmet": "^4.3.1",
    "JSONStream": "^1.3.5",
    "knex": "^0.21.15",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "micromustache": "^8.0.3",
    "mime-types": "^2.1.28",
