    "kafka-consumer-user-points": "ts-node ./bin/kafkaConsumer/userPointsConsumer/kafkaConsumer.ts",
    "kafka-consumer-exercises": "ts-node ./bin/kafkaConsumer/exerciseConsumer/kafkaConsumer.ts",
    "generate": "prisma generate && nexus-prisma-generate --client ./generated/prisma-client --output ./generated/nexus-prisma"
