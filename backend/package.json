{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node --transpile-only index.ts",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly index.ts",
    "build": "tsc",
    "upgrade-interactive": "npm-check --update",
    "prisma-deploy": "prisma deploy --force",
    "fetch-ai-completions": "ts-node ./bin/fetchAiCompletions.ts",
    "fetch-organizations": "ts-node ./bin/importOrganizations.ts",
    "fetch-user-app-datum": "ts-node ./bin/fetchUserAppDatum.ts",
    "fetch-user-field-values": "ts-node ./bin/fetchUserFieldValues.ts",
    "fetch-avoin-links": "ts-node ./bin/fetchAvoinLinks.ts",
    "send-ai-statistics": "ts-node ./bin/sendAiStatistics.ts",
    "import-organizations": "ts-node ./bin/importOrganizations.ts",
    "parse-file": "ts-node ./bin/parseFile.ts",
    "start-kafka-consumers": "concurrently npm:kafka-consumer-*",
    "kafka-consumer-user-course-progress": "ts-node --transpile-only ./bin/kafkaConsumer/userCourseProgressConsumer/kafkaConsumer.ts",
    "kafka-consumer-user-points": "ts-node  --transpile-only ./bin/kafkaConsumer/userPointsConsumer/kafkaConsumer.ts",
    "kafka-consumer-exercises": "ts-node  --transpile-only ./bin/kafkaConsumer/exerciseConsumer/kafkaConsumer.ts",
    "generate": "prisma generate && nexus-prisma-generate --client ./generated/prisma-client --output ./generated/nexus-prisma",
    "generate-nexus": "GOOGLE_CLOUD_STORAGE_BUCKET=x ts-node --transpile-only generateNexus",
    "seed": "ts-node -O '{\"module\": \"commonjs\"}' ./bin/seed.ts",
    "seedPoints": "ts-node -O '{\"module\": \"commonjs\"}' ./bin/seedPoints.ts",
    "createExports": "ts-node ./bin/createExports.ts",
    "kafka-bridge": "ts-node  --transpile-only './bin/kafkaBridge.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
