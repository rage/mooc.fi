    "@devoxa/prisma-relay-cursor-connection": "^2.2.1",
    "@google-cloud/storage": "^5.19.1",
    "@prisma/client": "2.23",
    "@sentry/integrations": "^6.19.6",
    "@sentry/node": "^6.19.6",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "graphql": "16.5.0",
    "graphql-scalars": "^1.17.0",
    "graphql-upload": "^13.0.0",
    "helmet": "^5.1.0",
    "JSONStream": "^1.3.5",
    "knex": "^2.1.0",
    "lodash": "^4.17.20",
    "luxon": "^2.4.0",
    "micromustache": "^8.0.3",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "newrelic": "^8.14.1",
    "nexus": "^1.3.0",
    "nexus-plugin-prisma": "^0.35.0",
    "node-rdkafka": "^2.13.0",
    "nodemailer": "^6.7.5",
    "patch-package": "^6.5.0",
    "pg": "^8.7.3",
    "pg-query-stream": "^4.2.3",
    "prisma": "^2.23.0",
    "redis": "^4.1.0",
    "sharp": "^0.30.6",
    "shortid": "^2.2.16",
    "uuid": "^8.3.2",
    "websocket": "^1.0.34",
    "winston": "^3.7.2",
    "winston-sentry-log": "^1.0.26",
    "yup": "^0.32.11"
  },
