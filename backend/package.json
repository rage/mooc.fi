    "newrelic": "^7.1.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.32.0",
    "node-rdkafka": "^2.10.1",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "pg-query-stream": "^4.0.0",
    "redis": "^3.0.2",
    "sharp": "^0.27.2",
    "shortid": "^2.2.16",
    "uuid": "^8.3.2",
    "websocket": "^1.0.33",
    "winston": "^3.3.3",
    "winston-sentry-log": "^1.0.19",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.18",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "@types/mime-types": "^2.1.0",
    "esm": "^3.2.25",
    "faker": "^5.1.0",
    "get-port": "^5.1.1",
    "graceful-fs": "^4.2.4",
    "graphql-request": "^3.4.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "nanoid": "^3.1.20",
    "nock": "^13.0.5",
    "prisma": "^2.18.0",
    "tmc-client-js": "0.0.9",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "graphql-upload": "11.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "testEnvironment": "node"
  }
}
