    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/faker": "^5.5.9",
    "@types/graphql-upload": "^8.0.12",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.190",
    "@types/luxon": "^3.1.0",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.6",
    "@types/shortid": "^0.0.29",
    "@types/uuid": "^8.3.4",
    "@types/websocket": "^1.0.5",
    "esm": "^3.2.25",
    "faker": "^5.5.3",
    "get-port": "^5.0.0",
    "graceful-fs": "^4.2.10",
    "graphql-request": "^4.3.0",
    "jest": "^29.3.1",
    "jest-junit": "^13.2.0",
    "jest-mock": "^28.1.1",
    "locate-path": "^7.1.1",
    "nanoid": "^3.3.2",
    "nock": "^13.2.7",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "overrides": {
    "graphql": "16.5.0"
  }
}
